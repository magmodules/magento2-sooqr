<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job instance="Magmodules\Sooqr\Cron\Feed" method="execute" name="sooqr_generate_feed">
            <schedule>0 0 * * *</schedule>
            <config_path>crontab/default/jobs/sooqr_data/general/cron_frequency</config_path>
        </job>
<!--        <job instance="Magmodules\Sooqr\Cron\Delta" method="execute" name="sooqr_generate_delta_feed">-->
<!--            <schedule>0 * * * *</schedule>-->
<!--            <config_path>crontab/default/jobs/sooqr_data/general/cron_frequency_delta</config_path>-->
<!--        </job>-->
        <job instance="Magmodules\Sooqr\Cron\Cleanup" method="execute" name="sooqr_cleanup_old_feeds">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
</config>
